swagger: '2.0'
info:
  description: 'This is a sample web app for bhs'
  version: 1.0.6
  title: Swagger Petstore
  termsOfService: http://swagger.io/terms/
  contact:
    email: apiteam@swagger.io
  license:
    name: Apache 2.0
    url: http://www.apache.org/licenses/LICENSE-2.0.html
host: petstore.swagger.io

definitions:
  User:
    type: object
    properties:
      id:
        type: integer
        format: int32
      username:
        type: string
      password:
        type: string
    xml:
      name: User
  Asset:
    type: object
    properties:
      id:
        type: integer
        format: int64
      name:
        type: string
      description:
        type: string
      price:
        type: integer
        format: int64
      owner_id:
        type: integer
        format: int64


externalDocs:
  description: Find out more about Swagger
  url: http://swagger.io
